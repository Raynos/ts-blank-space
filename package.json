{
    "name": "ts-blank-space",
    "version": "0.3.0",
    "license": "Apache-2.0",
    "type": "module",
    "main": "./out/index.js",
    "exports": {
        "./package.json": "./package.json",
        ".": "./out/index.js",
        "./register": "./loader/register.js"
    },
    "engines": {
        "node": ">=10.13"
    },
    "types": "./out/index.d.ts",
    "dependencies": {
        "typescript": "5.1.6 - 5.5.x"
    },
    "imports": {
        "#r": "./loader/register.js"
    },
    "devDependencies": {
        "@types/node": "^20.9.4",
        "prettier": "3.3.3"
    },
    "scripts": {
        "build": "node --import=#r ./scripts/build.ts",
        "gen-types": "tsc -p ./src/tsconfig.json",
        "fixtures": "node --import=#r ./tests/fixture/_run.ts",
        "fixtures:watch": "node --watch-path ./tests/fixture/cases --import=#r ./tests/fixture/_run.ts",
        "pretest": "npm run build",
        "test": "node --test --import=#r ./tests/*.test.ts ./tests/ecosystem/ecosystem.test.ts",
        "format": "prettier . -w",
        "format:check": "prettier . -c",
        "prepack": "npm run gen-types;npm run build;"
    },
    "files": [
        "!tests",
        "!perf",
        "out/*.js",
        "out/*.d.ts"
    ]
}
