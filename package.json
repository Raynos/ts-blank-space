{
  "name": "ts-blank-space",
  "version": "0.3.0",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./out/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": "./out/index.js",
    "./register": "./loader/register.js"
  },
  "engines": {
      "node": ">=10.13"
  },
  "types": "./out/index.d.ts",
  "dependencies": {
    "typescript": "5.1.6 - 5.5.x"
  },
  "devDependencies": {
    "@types/node": "^20.9.4"
  },
  "scripts": {
    "build": "node ./scripts/build.js",
    "build:watch": "node --watch-path ./src ./scripts/build.js",
    "gen-types": "tsc -p ./src/tsconfig.json",
    "fixtures": "node ./tests/fixture/_run.js",
    "fixtures:watch": "node --watch-path ./tests/fixture/cases --watch-path ./out ./tests/fixture/_run.js",
    "pretest": "npm run build",
    "test": "node --test ./tests/*.test.js ./tests/ecosystem/ecosystem.test.js",
    "prepack": "npm run gen-types;npm run build;"
  },
  "files": [
    "!tests",
    "!perf",
    "out/*.js",
    "out/*.d.ts"
  ]
}
