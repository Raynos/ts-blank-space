{
  "name": "ts-blank-space",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./src/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.js"
  },
  "engines": {
      "node": ">=10.13"
  },
  "types": "./types/index.d.ts",
  "dependencies": {
    "typescript": "5.1.6 - 5.5.x"
  },
  "devDependencies": {
    "@types/node": "^20.9.4"
  },
  "scripts": {
    "gen-types": "tsc -p jsconfig.json",
    "update-fixture": "node ./tests/fixture/_run.js",
    "update-fixture:watch": "node --watch-path ./src --watch-path ./tests/fixture/cases ./tests/fixture/_run.js",
    "test": "node --test ./tests/*.test.js ./tests/ecosystem/ecosystem.test.js",
    "prepack": "npm run gen-types"
  },
  "files": [
    "!tests",
    "!perf",
    "types/index.d.ts",
    "src/*.js"
  ]
}
